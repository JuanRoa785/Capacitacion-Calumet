      "cell_type": "code",
      "source": [
        "algorithms = [\n",
        "    ('kmeans', KMeans(n_clusters=2, n_init='auto')),\n",
        "    ('DBSCAN', DBSCAN(eps=0.5, min_samples=5)),\n",
        "    ('ACWard', AgglomerativeClustering(n_clusters=2, linkage=\"ward\")),\n",
        "    ('ACAverage', AgglomerativeClustering(n_clusters=2, linkage=\"average\")),\n",
        "    ('ACSingle', AgglomerativeClustering(n_clusters=2, linkage=\"single\")),\n",
        "]"
      ],
      "metadata": {
        "id": "582XT2iZYQAC"
